<aura:component implements="flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes"
    controller="AccountInteractionalLevelController">
    <aura:attribute name="recordId" type="Id" />
    <aura:attribute name="Acc" type="Object" />

    <aura:handler name="init" value="{! this }" action="{! c.init }" />


    <aura:if isTrue="{!and(v.Acc.Account_Interaction_Level__c!=null, v.Acc.RecordType.Name=='Bank' )}">
        <div class="slds-container--center slds-container--large"
            style="background: white;    height: 32px;    line-height: 30px;    padding: 0px 15px;    border: 1px solid rgb(221, 219, 218);">
            <div id="rightdiv">
                <div style="margin: 5px 9px 0px 0px; height: 20px; width: 20px; float: left;"
                    id="{!v.Acc.Account_Interaction_Level__c}"></div>
                <span>{!v.Acc.Account_Interaction_Level__c}</span>
            </div>
            <div id="leftdiv">Account Interaction Level: </div>
        </div>
    </aura:if>

</aura:component>