<aura:component controller="CapTableController">
    <!-- attributes -->
    <aura:attribute name="calculatedInvestorInfo" type="Object"/>
    <aura:attribute name="rawCapTableData" type="Object"/>
    <aura:attribute name="recordId" type="Id"/>

    <!-- handlers-->
    <aura:handler name="init" value="{!this}" action="{!c.init}"/>

    <lightning:card title="Account Editable Datatable">
        <lightning:datatable
                             aura:id="capDataTable"
                             columns="{! v.columns }"
                             data="{! v.data }"
                             keyField="Id"
                             onsave ="{!c.onSave}"
                             hideCheckboxColumn="true" />
    </lightning:card>
</aura:component>